<div class="container">
  <p id="notice"><%= notice %></p>

  <h1>Datasets</h1>

  <table class="table">
    <thead class="thead-light">
      <tr>
        <th>Dataset complete</th>
        <th>Dataset description</th>
        <th>Dataset doi</th>
        <th>Dataset enddate</th>
        <th>Dataset location</th>
        <th>Dataset name</th>
        <th>Dataset startdate</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @datasets.each do |dataset| %>
        <tr>
          <td><%= dataset.dataset_complete %></td>
          <td><%= dataset.dataset_description %></td>
          <td><%= dataset.dataset_doi %></td>
          <td><%= dataset.dataset_enddate %></td>
          <td><%= dataset.dataset_location %></td>
          <td><%= dataset.dataset_name %></td>
          <td><%= dataset.dataset_startdate %></td>
          <td><%= link_to 'Show', dataset %></td>
          <td><%= link_to 'Edit', edit_dataset_path(dataset) %></td>
          <td><%= link_to 'Destroy', dataset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Dataset', new_dataset_path %>
</div>
