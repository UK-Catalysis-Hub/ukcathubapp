    <%= geo_chart country_data, library: {backgroundColor: "#00FFFF", colors: ['#FF0000','#0000FF']}   %>
  </div>
  <div class="tab-pane fade" id="pills-profile" role="tabpanel" aria-labelledby="pills-profile-tab">
    <!-- Europe Map -->
    <%= geo_chart country_data, library: {backgroundColor: "#00FFFF", region: '150', colors: ['#FF0000','#0000FF']}   %>
  </div>
  <div class="tab-pane fade" id="pills-contact" role="tabpanel" aria-labelledby="pills-contact-tab">
    <!-- Asia Map -->
    <%= geo_chart country_data, library: {backgroundColor: "#00FFFF", region: '142', colors: ['#FF0000','#0000FF']}   %>
  </div>
